{
    "name": "web",
    "version": "0.0.0",
    "scripts": {
        "nx": "npx nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
        "start": "npx nx serve",
        "build": "npx nx build",
        "test": "npx nx run-many --target=test",
        "e2e": "npx nx e2e angular-strapi-e2e --browser chrome",
        "serve": "npx nx serve angular-strapi --host 0.0.0.0 --disable-host-check",
        "lint": "npx nx run-many --target=lint --format=stylish",
        "lint:style": "stylelint \"apps/**/*.scss\" && stylelint \"libs/**/*.scss\"",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "affected:test": "npx nx affected:test",
        "affected:lint": "npx nx affected:lint",
        "affected:e2e": "npx nx affected:e2e angular-strapi-e2e"
    },
    "private": true,
